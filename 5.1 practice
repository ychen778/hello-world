#basic list 1
list = []
for i in range(10):
    list.append(-113)
for i in range(len(list)):
    print (f"in slot {i} you have a {list[i]}")

#basic list 2
import random
list = []
for i in range(10):
    list.append(random.randrange(100))
for i in range(len(list)):
    print (f"in slot {i} there is a {list[i]}")

#basic list 3
import random
list = []
for i in range(1000):
    list.insert(1,random.randrange(10,99))
for num in list:
    print (num,end="  ")
    print ()

