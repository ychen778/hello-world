import random
lists = []
for i in range (10):
    lists.insert(1,random.randrange(1,50))
print (lists)
num = int(input("What number are you looking for? "))
for list in lists:
    if num in lists:
       print (f"{num} was in list")

import random
lists = []
times = 0
for i in range (10):
    lists.insert(1,random.randrange(1,50))
print (lists)
num = int(input("What number are you looking for? "))
for list in lists:
    if num == list:
       times += 1
print (f"{num} was found {times} times")

import random
lists = []
times = 0
found = False
for i in range (10):
    lists.insert(1,random.randrange(1,50))
print (lists)
num = int(input("What number are you looking for? "))
for list in lists:
    if num == list:
       found = True
if found == True:
    print (f"{num} was in the list")

import random
lists = []
for i in range (10):
    lists.insert(1,random.randrange(1,50))
print (lists)
num = int(input("Desired Value: "))
for i in range(len(lists)):
    number = lists[i]
    if num == number:
        print (f"found a {num} at {i}")
        print (i)

import random
list = []
for i in range(10):
    list.append(random.randrange(1,100))
print (list)
max_value = list[0]
for num in list:
    if num > max_value:
        max_value = num
print(max_value)

import random
list = []
for i in range(10):
    list.append(random.randrange(1,100))
print (list)
max_value = list[0]
for num in list:
    if num > max_value:
        max_value = num
index = list.index(max_value)
print (f"the largest number, {max_value} was found at {index}")
